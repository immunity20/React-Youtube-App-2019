{
    "plugins": ["react","jsx", "react-hooks", "emotion"],
    "extends": "airbnb",
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true,
      "import/extensions": [".js",".jsx",".ts",".tsx"],
       "import/resolver": {
           "node": {
               "extensions": [".js",".jsx",".ts",".tsx"]
           }
       }
    },
    "rules": {
      "react/jsx-filename-extension": [1, { 
        "extensions": [".js", ".jsx", ".tsx"] }]
    }
 }